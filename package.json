{
  "name": "qa-testing-system",
  "version": "1.0.0",
  "description": "PWA + Dashboard for AI Recommendations QA Testing",
  "main": "server-supabase.js",
  "scripts": {
    "start": "node server-supabase.js",
    "dev": "nodemon server.js",
    "debug": "node debug-server.js",
    "test-debug": "node test-debug.js",
    "prebuild": "npm --prefix pwa ci --include=dev && npm --prefix dashboard ci --include=dev",
    "build": "npm run build:pwa && npm run build:dashboard",
    "build:pwa": "cd pwa && npm run build",
    "build:dashboard": "cd dashboard && npm run build",
    "install:all": "npm install && cd pwa && npm install && cd ../dashboard && npm install"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.76.0",
    "axios": "^1.12.2",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^2.0.0",
    "sqlite3": "^5.1.6",
    "tslib": "^2.8.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  },
  "keywords": [
    "pwa",
    "qa",
    "testing",
    "ai",
    "recommendations"
  ],
  "author": "QA Testing System",
  "license": "MIT"
}
