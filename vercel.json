{
  "version": 2,
  "builds": [
    {
      "src": "server-debug.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server-debug.js"
    },
    {
      "src": "/(.*)",
      "dest": "/server-debug.js"
    }
  ],
  "functions": {
    "server-debug.js": {
      "maxDuration": 30
    }
  },
  "ignore": [
    "pwa/dist/**",
    "dashboard/dist/**",
    "public/**",
    "*.html",
    "*.css",
    "*.js",
    "*.json",
    "*.png",
    "*.svg",
    "*.webmanifest"
  ]
}