{
  "version": 2,
  "builds": [
    {
      "src": "server-supabase.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server-supabase.js"
    },
    {
      "src": "/manifest.json",
      "dest": "/server-supabase.js"
    },
    {
      "src": "/manifest.webmanifest", 
      "dest": "/server-supabase.js"
    },
    {
      "src": "/login",
      "dest": "/server-supabase.js"
    },
    {
      "src": "/admin(.*)",
      "dest": "/server-supabase.js"
    },
    {
      "src": "/(.*)",
      "dest": "/server-supabase.js"
    }
  ]
}